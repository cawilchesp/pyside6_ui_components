/* Window */
QWidget {
    background-color: hsl(0, 0%, 13%);
}

/* Card */
UI_Card {
    border-width: 0px;
    border-radius: 8px;
    background-color: hsl(0, 0%, 17%)
}

/* Labels */
UI_Label {
    border-width: 1px;
    border-style: solid;
    /* border-radius: 4px; */
    background-color: transparent;
    color: hsl(0, 0%, 98%);
    padding: 0px
}
UI_IconLabel {
    border-width: 0px;
    background-color: transparent;
    padding: 4px
}
UI_ColorLabel {
    border-width: 2px;
    border-style: solid;
    border-color: hsl(200, 100%, 50%);
    border-radius: 4px;
    padding: 0px
}

/* Button */
UI_Button {
    border-radius: 4px;
    margin: 4px
}
UI_Button[type=standard] {
    background-color: hsl(0, 0%, 20%);
    color: hsl(0, 0%, 98%)
}
UI_Button[type=accent] {
    background-color: hsl(200, 100%, 50%);
    color: hsl(0, 0%, 13%)
}
UI_Button[type=outlined] {
    border-width: 2px;
    border-style: solid;
    border-color: hsl(200, 100%, 50%);
    background-color: transparent;
    color: hsl(200, 100%, 50%);
}
UI_Button[type=hyperlink] {
    background-color: transparent;
    color: hsl(200, 100%, 50%)
}
UI_Button:hover {
    background-color: hsl(0, 0%, 25%);
}
UI_Button:hover[type=accent] {
    background-color: hsl(200, 100%, 40%);
}
UI_Button:disabled {
    background-color: hsl(0, 0%, 16%);
    color: hsl(0, 0%, 35%)
}
UI_Button:disabled[type=outlined] {
    border-color: hsl(0, 0%, 35%)
}

UI_ToggleButton {
    border-radius: 4px;
    background-color: hsl(0, 0%, 20%);
    color: hsl(0, 0%, 98%);
    margin: 4px
}
UI_ToggleButton:checked {
    background-color: hsl(200, 100%, 50%);
    color: hsl(0, 0%, 13%)
}
UI_ToggleButton:hover {
    background-color: hsl(0, 0%, 25%);
}
UI_ToggleButton:checked:hover {
    background-color: hsl(200, 100%, 40%);
}
UI_ToggleButton:disabled {
    background-color: hsl(0, 0%, 16%);
    color: hsl(0, 0%, 35%)
}

UI_ThemeButton {
    border-width: 0px;
    border-radius: 4px;
    background-color: hsl(200, 100%, 50%);
    color: hsl(0, 0%, 13%);
    margin: 4px
}
UI_ThemeButton:hover {
    background-color: hsl(200, 100%, 40%);
}
UI_ThemeButton:disabled {
    background-color: hsl(0, 0%, 16%);
    color: hsl(0, 0%, 35%)
}

UI_DropDownButton {
    border-radius: 4px;
    background-color: hsl(0, 0%, 20%);
    color: hsl(0, 0%, 98%);
    padding-left: 12px;
    padding-right: 40px;
    margin: 4px
}
UI_DropDownButton:disabled {
    background-color: hsl(0, 0%, 16%);
    color: hsl(0, 0%, 35%)
}
UI_DropDownButton::menu-button {
    border-radius: 4px;
    subcontrol-origin: border;
    subcontrol-position: center right;
    width: 32;
    height: 22;
    right: 4px;
    background-color: transparent
}
UI_DropDownButton::menu-button:hover {
    background-color: hsl(0, 0%, 25%)
}
UI_DropDownButton::menu-arrow {
    width: 20px;
    height: 20px;
    image: url(icons/chevron_down_white.png)
}
UI_DropDownButton QMenu {
    border-width: 4px;
    border-style: solid;
    border-color: hsl(0, 0%, 20%);
    border-radius: 8px;
    background-color: hsl(0, 0%, 17%)
}
UI_DropDownButton QMenu::item {
    background-color: hsl(0, 0%, 20%);
    color: hsl(0, 0%, 98%);
    height: 32px;
    padding-left: 12px
}
UI_DropDownButton QMenu::item::selected {
    border-color: hsl(0, 0%, 20%);
    border-radius: 4px;
    background-color: hsl(0, 0%, 25%);
    padding-left: 12px
}

/* Check Box */
UI_CheckBox {
    background-color: transparent;
    color: hsl(0, 0%, 98%);
    margin: 4px
}
UI_CheckBox:disabled {
    background-color: hsl(0, 0%, 16%);
    color: hsl(0, 0%, 35%)
}
UI_CheckBox::indicator {
    border-width: 1px;
    border-style: solid;
    border-color: hsl(0, 0%, 90%);
    border-radius: 4px;
    width: 20px;
    height: 20px;
}
UI_CheckBox::indicator:unchecked {
    background-color: hsl(0, 0%, 17%);
}
UI_CheckBox::indicator:unchecked:hover {
    background-color: hsl(0, 0%, 25%);
}
UI_CheckBox::indicator:checked {
    background-color: hsl(200, 100%, 50%);
    image: url(icons/done_black.png)
}
UI_CheckBox::indicator:checked:hover {
    background-color: hsl(200, 100%, 40%);
}
UI_CheckBox::indicator:indeterminate {
    background-color: hsl(200, 100%, 50%);
    image: url(icons/minus_black.png)
}
UI_CheckBox::indicator:indeterminate:hover {
    background-color: hsl(200, 100%, 40%);
}

/* Combo Box */
UI_ComboBox {
    border-width: 0px;
    border-radius: 4px;
    padding-left: 12px;
    padding-right: 40px;
    background-color: hsl(0, 0%, 20%);
    color: hsl(0, 0%, 98%);
    margin: 4px;
}
UI_ComboBox:editable {
    border-bottom-width: 2px;
    border-bottom-style: solid;
    border-bottom-color: hsl(200, 100%, 50%);
}
UI_ComboBox:disabled {
    background-color: hsl(0, 0%, 16%);
    color: hsl(0, 0%, 35%)
}
UI_ComboBox::drop-down {
    border-radius: 4px;
    subcontrol-origin: border;
    subcontrol-position: center right;
    width: 32;
    height: 22;
    right: 4px;
    background-color: transparent
}
UI_ComboBox::drop-down:hover {
    background-color: hsl(0, 0%, 25%)
}
UI_ComboBox::down-arrow {
    width: 16;
    height: 16;
    image: url(icons/chevron_down_white.png);
}
UI_ComboBox QListView {
    color: hsl(0, 0%, 98%);
    margin: 0px;
    outline: 0px;
}
UI_ComboBox QListView:active {
    border-width: 3px;
    border-style: solid;
    border-color: hsl(0, 0%, 17%);
    border-radius: 10px;
    background-color: hsl(0, 0%, 20%);
    margin: -3px;
}
UI_ComboBox QListView::item {
    height: 24;
}
UI_ComboBox QListView::item:selected {
    border-left-width: 4px;
    border-left-style: solid;
    border-left-color: hsl(200, 100%, 50%);
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    background-color: hsl(0, 0%, 25%);
    color: hsl(0, 0%, 98%);
    margin: 4px;
}
UI_ComboBox QListView::item:!selected {
    border-width: 4px;
    border-style: solid;
    border-color: hsl(0, 0%, 20%);
    margin: 4px
}

/* Radio Button */
UI_RadioButton {
    background-color: transparent;
    color: hsl(0, 0%, 98%);
    margin: 4px
}
UI_RadioButton:disabled {
    background-color: hsl(0, 0%, 16%);
    color: hsl(0, 0%, 35%)
}
UI_RadioButton::indicator {
    border-width: 1px;
    border-style: solid;
    border-color: hsl(0, 0%, 90%);
    border-radius: 10px;
    width: 18px;
    height: 18px;
}
UI_RadioButton::indicator:unchecked {
    background-color: hsl(0, 0%, 17%);
}
UI_RadioButton::indicator:unchecked:hover {
    background-color: hsl(0, 0%, 25%);
}
UI_RadioButton::indicator:checked {
    border-width: 4px;
    border-style: solid;
    border-color: hsl(200, 100%, 50%);
    border-radius: 10px;
    width: 12px;
    height: 12px;
}
UI_RadioButton::indicator:checked:hover {
    border-width: 3px;
    border-style: solid;
    border-color: hsl(200, 100%, 50%);
    border-radius: 10px;
    width: 14px;
    height: 14px;
}

/* Slider */
UI_Slider { 
    background-color: transparent;
    margin: 4px;
}
UI_Slider::groove:horizontal {
    background: hsl(200, 100%, 50%);
    height: 4px
}
UI_Slider::groove:vertical {
    background: hsl(200, 100%, 50%);
    width: 4px
}
UI_Slider::handle {
    border-width: 5px;
    border-style: solid;
    border-color: hsl(0, 0%, 30%);
    border-radius: 10px;
    background-color: hsl(200, 100%, 50%);
    width: 10px;
    height: 10px;
    margin: -8px
}
UI_Slider::handle:hover {
    border-width: 3px;
    border-style: solid;
    border-color: hsl(0, 0%, 30%);
    border-radius: 10px;
    background-color: hsl(200, 100%, 50%);
    width: 14px;
    height: 14px;
    margin: -8px
}
UI_Slider::handle:disabled {
    background-color: hsl(0, 0%, 20%)
}
UI_Slider::add-page:horizontal {
    background: hsl(0, 0%, 60%)
}
UI_Slider::sub-page:vertical {
    background: hsl(0, 0%, 60%)
}
UI_Slider::add-page:vertical {
    background: hsl(200, 100%, 50%)
}
UI_Slider::sub-page:horizontal {
    background: hsl(200, 100%, 50%)
}
UI_Slider::add-page:disabled {
    background: hsl(0, 0%, 25%)
}
UI_Slider::sub-page:disabled {
    background: hsl(0, 0%, 25%)
}

/* Switch */
UI_Switch {
    background-color: transparent;
}
UI_Switch QToolButton {
    background-color: transparent;
    color: hsl(0, 0%, 90%);
    padding-bottom: 0px
}
UI_Switch QToolButton:hover {
    font-size: 14px;
}
UI_Switch QToolButton[side=left] {
    border-width: 1px;
    border-style: solid;
    border-color: hsl(0, 0%, 90%);
    border-right-width: 0px;
    border-top-left-radius: 10;
    border-bottom-left-radius: 10;
}
UI_Switch QToolButton[side=right] {
    border-width: 1px;
    border-style: solid;
    border-color: hsl(0, 0%, 90%);
    border-left-width: 0px;
    border-top-right-radius: 10;
    border-bottom-right-radius: 10;
}
UI_Switch QToolButton:checked {
    background-color: hsl(200, 100%, 50%);
}
UI_Switch QToolButton:disabled {
    background-color: hsl(0, 0%, 71%)
}

