/* Window */
QWidget {
    background-color: @background_full;
}

/* Card */
UI_Card {
    border-width: 0px;
    border-radius: 8px;
    background-color: @background_card;
}

/* Labels */
UI_Label {
    border-width: 0px;
    background-color: transparent;
    color: @text_active;
    padding: 0px;
}
UI_IconLabel {
    border-width: 0px;
    background-color: transparent;
    padding: 4px;
}
UI_ColorLabel {
    border-width: 2px;
    border-style: solid;
    border-color: @theme_active;
    border-radius: 4px;
    padding: 0px;
}

/* Button */
UI_Button {
    border-radius: 4px;
    margin: 4px;
    font-family: "Segoe Fluent Icons";
    font-size: 16px;
}
UI_Button[type=standard] {
    background-color: @background_widget;
    color: @text_active;
}
UI_Button[type=accent] {
    background-color: @theme_active;
    color: @background_full;
}
UI_Button[type=outlined] {
    border-width: 2px;
    border-style: solid;
    border-color: @theme_active;
    background-color: transparent;
    color: @theme_active;
}
UI_Button[type=hyperlink] {
    background-color: transparent;
    color: @theme_active;
}
UI_Button:hover {
    background-color: @background_hover;
}
UI_Button:hover[type=accent] {
    background-color: @theme_hover;
}
UI_Button:disabled {
    background-color: @background_disable;
    color: @text_disable;
}
UI_Button:disabled[type=outlined] {
    border-color: @text_disable;
}

UI_ToggleButton {
    border-radius: 4px;
    background-color: @background_widget;
    color: @text_active;
    margin: 4px;
    font-family: "Segoe Fluent Icons";
    font-size: 16px;
}
UI_ToggleButton:checked {
    background-color: @theme_active;
    color: @background_full;
}
UI_ToggleButton:hover {
    background-color: @background_hover;
}
UI_ToggleButton:checked:hover {
    background-color: @theme_hover;
}
UI_ToggleButton:disabled {
    background-color: @background_disable;
    color: @text_disable;
}

UI_ThemeButton {
    border-width: 0px;
    border-radius: 4px;
    background-color: @theme_active;
    color: @background_full;
    margin: 4px;
    font-family: "Segoe Fluent Icons";
    font-size: 16px;
}
UI_ThemeButton:hover {
    background-color: @theme_hover;
}
UI_ThemeButton:disabled {
    background-color: @background_disable;
    color: @text_disable;
}

UI_DropDownButton {
    border-radius: 4px;
    background-color: @background_widget;
    color: @text_active;
    padding-left: 12px;
    padding-right: 40px;
    margin: 4px;
    font-family: "Segoe Fluent Icons";
    font-size: 16px;
}
UI_DropDownButton:disabled {
    background-color: @background_disable;
    color: @text_disable;
}
UI_DropDownButton::menu-button {
    border-radius: 4px;
    subcontrol-origin: border;
    subcontrol-position: center right;
    width: 32;
    height: 22;
    right: 4px;
    background-color: transparent;
}
UI_DropDownButton::menu-button:hover {
    background-color: @background_hover;
}
UI_DropDownButton::menu-arrow {
    width: 20px;
    height: 20px;
    image: url(icons/@chevron_down);
}
UI_DropDownButton QMenu {
    border-width: 4px;
    border-style: solid;
    border-color: @background_widget;
    border-radius: 8px;
    background-color: @background_card;
    font-family: "Segoe Fluent Icons";
    font-size: 16px;
}
UI_DropDownButton QMenu::item {
    background-color: @background_widget;
    color: @text_active;
    height: 32px;
    padding-left: 12px;
}
UI_DropDownButton QMenu::item::selected {
    border-color: @background_widget;
    border-radius: 4px;
    background-color: @background_hover;
    padding-left: 12px;
}

/* Check Box */
UI_CheckBox {
    background-color: transparent;
    color: @text_active;
    margin: 4px;
    font-family: "Segoe Fluent Icons";
    font-size: 16px;
}
UI_CheckBox:disabled {
    background-color: @background_disable;
    color: @text_disable;
}
UI_CheckBox::indicator {
    border-width: 1px;
    border-style: solid;
    border-color: @text_active;
    border-radius: 4px;
    width: 20px;
    height: 20px;
}
UI_CheckBox::indicator:unchecked {
    background-color: @background_card;
}
UI_CheckBox::indicator:unchecked:hover {
    background-color: @background_hover;
}
UI_CheckBox::indicator:checked {
    background-color: @theme_active;
    image: url(icons/@done);
}
UI_CheckBox::indicator:checked:hover {
    background-color: @theme_hover;
}
UI_CheckBox::indicator:indeterminate {
    background-color: @theme_active;
    image: url(icons/@minus);
}
UI_CheckBox::indicator:indeterminate:hover {
    background-color: @theme_hover;
}

/* Combo Box */
UI_ComboBox {
    border-width: 0px;
    border-radius: 4px;
    padding-left: 12px;
    padding-right: 4px;
    background-color: @background_widget;
    color: @text_active;
    margin: 4px;
    font-family: "Segoe Fluent Icons";
    font-size: 16px;
}
UI_ComboBox:editable {
    border-bottom-width: 2px;
    border-bottom-style: solid;
    border-bottom-color: @theme_active;
}
UI_ComboBox:disabled {
    background-color: @background_disable;
    color: @text_disable;
}
UI_ComboBox::drop-down {
    border-radius: 4px;
    subcontrol-origin: border;
    subcontrol-position: center right;
    width: 32;
    height: 22;
    right: 4px;
    background-color: transparent
}
UI_ComboBox::drop-down:hover {
    background-color: @background_hover;
}
UI_ComboBox::down-arrow {
    width: 16;
    height: 16;
    image: url(icons/@chevron_down);
}
UI_ComboBox QListView {
    color: @text_active;
    margin: 0px;
    outline: 0px;
    font-family: "Segoe Fluent Icons";
    font-size: 16px;
}
UI_ComboBox QListView:active {
    border-width: 3px;
    border-style: solid;
    border-color: @background_widget;
    border-radius: 10px;
    background-color: @background_card;
    margin: -3px;
}
UI_ComboBox QListView::item {
    height: 24;
}
UI_ComboBox QListView::item:selected {
    border-left-width: 4px;
    border-left-style: solid;
    border-left-color: @theme_active;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    background-color: @background_hover;
    color: @text_active;
    margin: 4px;
}
UI_ComboBox QListView::item:!selected {
    border-width: 4px;
    border-style: solid;
    border-color: @background_widget;
    margin: 4px;
}

/* Radio Button */
UI_RadioButton {
    background-color: transparent;
    color: @text_active;
    margin: 4px;
    font-family: "Segoe Fluent Icons";
    font-size: 16px;
}
UI_RadioButton:disabled {
    background-color: @background_disable;
    color: @text_disable;
}
UI_RadioButton::indicator {
    border-width: 1px;
    border-style: solid;
    border-color: @text_active;
    border-radius: 10px;
    width: 18px;
    height: 18px;
}
UI_RadioButton::indicator:unchecked {
    background-color: @background_card;
}
UI_RadioButton::indicator:unchecked:hover {
    background-color: @background_hover;
}
UI_RadioButton::indicator:checked {
    border-width: 4px;
    border-style: solid;
    border-color: @theme_active;
    border-radius: 10px;
    width: 12px;
    height: 12px;
}
UI_RadioButton::indicator:checked:hover {
    border-width: 3px;
    border-style: solid;
    border-color: @theme_active;
    border-radius: 10px;
    width: 14px;
    height: 14px;
}

/* Slider */
UI_Slider { 
    background-color: transparent;
    margin: 4px;
}
UI_Slider::groove:horizontal {
    background: @theme_active;
    height: 4px;
}
UI_Slider::groove:vertical {
    background: @theme_active;
    width: 4px;
}
UI_Slider::handle {
    border-width: 5px;
    border-style: solid;
    border-color: @border_handle;
    border-radius: 10px;
    background-color: @theme_active;
    width: 10px;
    height: 10px;
    margin: -8px;
}
UI_Slider::handle:hover {
    border-width: 3px;
    border-style: solid;
    border-color: @border_handle;
    border-radius: 10px;
    background-color: @theme_active;
    width: 14px;
    height: 14px;
    margin: -8px;
}
UI_Slider::handle:disabled {
    background-color: @background_widget
}
UI_Slider::add-page:horizontal {
    background: @slider_page;
}
UI_Slider::sub-page:vertical {
    background: @slider_page;
}
UI_Slider::add-page:vertical {
    background: @theme_active;
}
UI_Slider::sub-page:horizontal {
    background: @theme_active;
}
UI_Slider::add-page:disabled {
    background: @background_hover;
}
UI_Slider::sub-page:disabled {
    background: @background_hover;
}

/* Switch */
UI_Switch {
    background-color: transparent;
}
UI_Switch QToolButton {
    background-color: transparent;
    color: @text_active;
    padding-bottom: 0px
}
UI_Switch QToolButton:hover {
    font-size: 14px;
}
UI_Switch QToolButton[side=left] {
    border-width: 1px;
    border-style: solid;
    border-color: @text_active;
    border-right-width: 0px;
    border-top-left-radius: 10;
    border-bottom-left-radius: 10;
}
UI_Switch QToolButton[side=right] {
    border-width: 1px;
    border-style: solid;
    border-color: @text_active;
    border-left-width: 0px;
    border-top-right-radius: 10;
    border-bottom-right-radius: 10;
}
UI_Switch QToolButton:checked {
    background-color: @theme_active;
}
UI_Switch QToolButton:disabled {
    background-color: @background_disable;
}

